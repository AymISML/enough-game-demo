{
    "routes": [
        {
            "src": "/Build/(.*)\\.js",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable",
                "Content-Encoding": "br"
            },
            "dest": "/Build/$1.js.br"
        },
        {
            "src": "/Build/(.*)\\.wasm",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable",
                "Content-Encoding": "br"
            },
            "dest": "/Build/$1.wasm.br"
        },
        {
            "src": "/Build/(.*)\\.data",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable",
                "Content-Encoding": "br"
            },
            "dest": "/Build/$1.data.br"
        }
    ]
}